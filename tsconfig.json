{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    /* Alias */
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@uils/*": [
        "src/uils/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@themes/*": [
        "src/styles/themes/*"
      ]
    }
  }
}